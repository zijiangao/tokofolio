<body class="text-center" >
    
    <div class="container login">
        <form class="form-signin" method="POST" action="/users/login">
            <img class="mb-4" src="/../tokofolio_logo1.svg" alt="200" width="200">
            <h1 class="h3 mb-3 font-weight-normal" id= title>Please Sign In</h1>
            <div class="form-group">
                <!-- <label for="email">Email</label> -->
                <label for="inputEmail" class="sr-only">Email</label>
                <!-- <input type="email" class="form-control" id="email" name="email" /> -->
                <input type="email" class="form-control" placeholder="Email address" required="" autofocus="" id='email' name='email'>
            </div>

            <div class="form-group">
                <label for="password" class="sr-only">Password</label>
                <!-- <input type="password" class="form-control" id="password" name="password" /> -->
                <input type="password" class="form-control" placeholder="Password" required="" id='password' name='password'>
                <div class="checkbox mb-3">
                  </div>
            </div>
            <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
            <!-- <button type="submit" class="btn btn-primary">Submit</button> -->
          </form> 
            
     </div>       
</body>
